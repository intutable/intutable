{
    "name": "intutable-org",
    "version": "0.2.0",
    "private": true,
    "engines": {
        "node": ">=18.16.0 <19.0.0",
        "npm": ">=9.0.0"
    },
    "workspaces": [
        "apps/*",
        "libs/plugins/*",
        "libs/core",
        "libs/shared",
        "libs/dekanat-app-plugin",
        "./.config/eslint"
    ],
    "scripts": {
        "db:start": "docker-compose start dekanat-database",
        "docs:open": "mdbook serve ./docs --open",
        "docs:test": "mdbook test ./docs",
        "prepare": "husky install",
        "lint": "npm run lint --workspaces --if-present -- --cache",
        "format": "npm run format --workspaces --if-present -- --cache --fix"
    },
    "dependencies": {
        "date-fns": "^2.29.1",
        "next": "^12.3.4",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^18.4.3",
        "@commitlint/config-conventional": "^18.4.3",
        "@types/jest": "^27.4.1",
        "@types/uuid": "^9.0.1",
        "@typescript-eslint/eslint-plugin": "^6.16.0",
        "@typescript-eslint/parser": "^6.16.0",
        "@vercel/style-guide": "^5.1.0",
        "concurrently": "^7.3.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.56.0",
        "eslint-config-next": "12.1.5",
        "eslint-config-prettier": "^9.0.0",
        "husky": "^8.0.3",
        "jest": "^27.1.4",
        "madge": "^5.0.1",
        "nodemon": "^2.0.19",
        "prettier": "3.x",
        "ts-jest": "^27.1.4",
        "ts-node": "^10.9.1",
        "typescript": "^4.6.3"
    }
}
