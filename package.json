{
    "name": "dekanat-app",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "build": "npm run build-core && next build",
        "dev": "node ./backend/dist/start/index dev",
        "start": "node ./backend/dist/start start",
        "build-core": "cd backend/src && tsc && cd ../..",
        "start-core": "node ./backend/dist/start/core",
        "dev-next": "next dev",
        "build-next": "next build",
        "start-next": "next start",
        "db:setup": "npx rimraf data.db && sqlite3 data.db < backend/database/createDB.sql",
        "lint": "next lint --no-cache"
    },
    "dependencies": {
        "@emotion/react": "^11.4.1",
        "@emotion/server": "^11.4.0",
        "@emotion/styled": "^11.3.0",
        "@intutable/core": "^0.1.3",
        "@intutable/database": "^1.1.1",
        "@intutable/http": "^0.1.4",
        "@intutable/project-management": "^0.1.0",
        "@mui/icons-material": "^5.0.1",
        "@mui/material": "^5.0.2",
        "@mui/styles": "^5.0.1",
        "@mui/x-data-grid": "^5.0.0-beta.2",
        "cors": "2.8.5",
        "next": "12.0.1",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "typescript": "^4.4.4"
    },
    "devDependencies": {
        "@types/node": "16.11.6",
        "@types/react": "17.0.33",
        "eslint": "^7.32.0",
        "eslint-config-next": "12.0.3",
        "eslint-config-prettier": "^8.3.0",
        "rimraf": "3.0.2"
    }
}
