{
    "name": "dekanat-app",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "build": "npm run build-core && next build",
        "dev": "npm run start-db & npm run build-core && node ./dist/start dev",
        "start": "node ./dist/start start",
        "build-core": "cd src/backend && tsc && cd ../..",
        "start-core": "node ./dist/start/core",
        "build-db": "cd database/ && docker-compose build --pull",
        "start-db": "npm run build-db && cd database/ && docker-compose up",
        "remove-db-data": "cd database/ && docker-compose down && cd .. && docker volume rm database_db-data",
        "dev-next": "next dev",
        "build-next": "next build",
        "start-next": "next start",
        "lint": "next lint",
        "lint:fix": "npm run lint -- --fix",
        "ci": "npm run lint && npm run build"
    },
    "dependencies": {
        "14": "^3.1.6",
        "@emotion/css": "^11.7.1",
        "@emotion/react": "^11.4.1",
        "@emotion/server": "^11.4.0",
        "@emotion/styled": "^11.3.0",
        "@intutable/core": "^0.1.6",
        "@intutable/database": "1.3.0",
        "@intutable/http": "^0.1.5",
        "@intutable/http-cors": "0.1.0",
        "@intutable/join-tables": "1.3.0",
        "@intutable/project-management": "0.4.10",
        "@intutable/user-authentication": "0.1.3",
        "@mui/base": "^5.0.0-alpha.65",
        "@mui/icons-material": "^5.0.1",
        "@mui/lab": "^5.0.0-alpha.75",
        "@mui/material": "^5.0.2",
        "@mui/styles": "^5.0.1",
        "@mui/system": "^5.2.1",
        "clsx": "^1.1.1",
        "cors": "2.8.5",
        "next": "^12.0.9",
        "notistack": "^2.0.3",
        "react": "^18.0.0-rc.0",
        "react-data-grid": "^7.0.0-beta.7",
        "react-dnd": "^15.1.1",
        "react-dnd-html5-backend": "^15.1.2",
        "react-dom": "^18.0.0-rc.0",
        "swr": "^1.2.1",
        "typescript": "^4.4.4"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "^12.0.10",
        "@types/cors": "^2.8.12",
        "@types/js-cookie": "^3.0.1",
        "@types/node": "16.11.6",
        "@types/react": "17.0.33",
        "@typescript-eslint/eslint-plugin": "^5.10.2",
        "eslint": "^7.32.0",
        "eslint-config-next": "12.0.3",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.28.0",
        "prettier": "^2.5.1",
        "rimraf": "3.0.2"
    }
}
