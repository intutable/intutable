{
    "name": "dekanat-app",
    "version": "0.1.0-alpha.3",
    "private": true,
    "scripts": {
        "build": "npm run build:core && next build",
        "dev": "npm run start:db & npm run build:core && node ./src/backend/startup/dist dev",
        "start": "npm run start:db & npm run build:core && node ./src/backend/startup/dist start",
        "build:core": "cd src/backend/startup && tsc && cd ../dekanat-app-plugin && tsc && cd ../../..",
        "start:core": "node ./src/backend/startup/dist/core",
        "build:db": "cd database/ && docker-compose build --pull",
        "start:db": "npm run build:db && cd database/ && docker-compose up",
        "reset-db": "cd database/ && docker-compose down && cd .. && docker volume rm database_db-data",
        "dev:next": "next dev",
        "build:next": "next build",
        "start:next": "next start",
        "lint": "NODE_ENV=development next lint",
        "lint:fix": "npm run lint -- --fix",
        "lint:production": "NODE_ENV=production next lint",
        "test": "jest",
        "analyze": "cross-env ANALYZE=true next build"
    },
    "dependencies": {
        "@emotion/css": "^11.9.0",
        "@emotion/react": "^11.9.0",
        "@emotion/server": "^11.4.0",
        "@emotion/styled": "^11.8.1",
        "@intutable/core": "^0.1.6",
        "@intutable/database": "^1.3.0",
        "@intutable/http": "^0.1.5",
        "@intutable/http-cors": "^0.1.0",
        "@intutable/lazy-views": "^3.0.2",
        "@intutable/project-management": "0.4.10",
        "@intutable/user-authentication": "^0.1.3",
        "@mui/base": "^5.0.0-alpha.76",
        "@mui/icons-material": "^5.6.1",
        "@mui/lab": "^5.0.0-alpha.77",
        "@mui/material": "^5.0.2",
        "@mui/styles": "^5.0.1",
        "@mui/system": "^5.2.1",
        "@mui/x-date-pickers": "^5.0.0-beta.4",
        "clsx": "^1.1.1",
        "cors": "^2.8.5",
        "date-fns": "^2.29.1",
        "fs-extra": "^10.1.0",
        "iron-session": "^6.1.1",
        "json2csv": "^5.0.7",
        "next": "^12.1.5",
        "next-transpile-modules": "^9.0.0",
        "notistack": "^2.0.4",
        "react": "^18.0.0",
        "react-data-grid": "^7.0.0-beta.12",
        "react-dnd": "^16.0.0",
        "react-dnd-html5-backend": "^16.0.0",
        "react-dom": "^18.0.0",
        "react-spring": "^9.4.5",
        "swr": "^1.3.0",
        "tmp": "^0.2.1",
        "typescript": "^4.6.3"
    },
    "devDependencies": {
        "@mui/codemod": "^5.8.5",
        "@next/bundle-analyzer": "^12.2.0",
        "@types/cors": "^2.8.12",
        "@types/fs-extra": "^9.0.13",
        "@types/jest": "^27.4.1",
        "@types/js-cookie": "^3.0.1",
        "@types/json2csv": "^5.0.3",
        "@types/node": "^17.0.24",
        "@types/react": "^18.0.5",
        "@types/tmp": "^0.2.3",
        "@typescript-eslint/eslint-plugin": "^5.10.2",
        "cross-env": "^7.0.3",
        "eslint": "^8.18.0",
        "eslint-config-next": "12.1.5",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.28.0",
        "jest": "^27.1.4",
        "prettier": "^2.5.1",
        "rimraf": "3.0.2",
        "ts-jest": "^27.1.4"
    }
}
