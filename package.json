{
    "name": "dekanat-app",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "build": "npm run build-core && next build",
        "dev": "npm run build-core && npm run db:setup && node ./dist/start/index dev",
        "start": "node ./dist/start start",
        "build-core": "cd src/backend && tsc && cd ../..",
        "start-core": "node ./dist/start/core",
        "dev-next": "next dev",
        "build-next": "next build",
        "start-next": "next start",
        "db:setup": "npx rimraf data.db && sqlite3 data.db < database/createDB.sql",
        "lint": "next lint"
    },
    "dependencies": {
        "@emotion/react": "^11.4.1",
        "@emotion/server": "^11.4.0",
        "@emotion/styled": "^11.3.0",
        "@intutable/core": "^0.1.6",
        "@intutable/database": "^1.2.2",
        "@intutable/http": "^0.1.5",
        "@intutable/http-cors": "0.1.0",
        "@intutable/project-management": "0.3.7",
        "@intutable/user-authentication": "0.1.3",
        "@mui/base": "^5.0.0-alpha.65",
        "@mui/icons-material": "^5.0.1",
        "@mui/material": "^5.0.2",
        "@mui/styles": "^5.0.1",
        "@mui/system": "^5.2.1",
        "@mui/x-data-grid": "^5.0.0-beta.2",
        "cors": "2.8.5",
        "js-cookie": "^3.0.1",
        "next": "^12.0.9",
        "notistack": "^2.0.3",
        "react": "^18.0.0-rc.0",
        "react-data-grid": "^7.0.0-beta.7",
        "react-dom": "^18.0.0-rc.0",
        "typescript": "^4.4.4"
    },
    "devDependencies": {
        "@types/cors": "^2.8.12",
        "@types/js-cookie": "^3.0.1",
        "@types/node": "16.11.6",
        "@types/react": "17.0.33",
        "eslint": "^7.32.0",
        "eslint-config-next": "12.0.3",
        "eslint-config-prettier": "^8.3.0",
        "rimraf": "3.0.2"
    }
}
